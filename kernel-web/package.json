{
  "name": "kernel-web",
  "version": "0.1.0",
  "description": "Web-based Kernel MDX chat application",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "npm run build:client && nodemon --exec ts-node src/server.ts",
    "dev:watch": "concurrently \"nodemon --watch src/client --exec 'npm run build:client'\" \"nodemon --exec ts-node src/server.ts\"",
    "build": "tsc && webpack --mode production",
    "build:client": "webpack --mode production",
    "build:server": "tsc",
    "watch": "tsc --watch"
  },
  "keywords": [
    "kernel",
    "mdx",
    "ai",
    "chat"
  ],
  "author": "yurysudz",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@babel/core": "^7.27.4",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "babel-loader": "^10.0.0",
    "chokidar": "^3.5.3",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "glob": "^11.0.3",
    "html-webpack-plugin": "^5.6.3",
    "marked": "^9.1.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.2",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.0.3",
    "concurrently": "^9.1.2",
    "css-loader": "^7.1.2",
    "nodemon": "^3.0.2",
    "style-loader": "^4.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "webpack-dev-server": "^5.2.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
